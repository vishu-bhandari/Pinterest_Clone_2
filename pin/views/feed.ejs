<% include ./partials/header.ejs %>


    <div class="w-full min-h-screen bg-zinc-800 px-10 py-8 text-white">
        <div class="flex flex-wrap cards gap-10">
            <% posts.forEach(function(post){ %>

                <div class=" card w-32">
                    <div class=" w-full h-40 bg-white rounded-lg overflow-hidden">
                        <img src="/images/uploads/<%= post.image%>" alt="" class="w-full h-full object-cover">
                    </div>
                    <h5 class="mt-2 text-sm "><%= post.title%> </h5>
                    <h3 class="leading-3 text-xs"><%= post.description %></h3>
    
    
                </div>
          <%  }) %>

        </div>
    </div>

    <% include ./partials/footer.ejs %>